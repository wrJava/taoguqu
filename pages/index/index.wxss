/**index.wxss**/

.headerBg {
  width: 100%;
  height: 208rpx;
  position: absolute;
  top: 0;
  left: 0;
}

.center {
  width: 88%;
  box-shadow: 8rpx 8rpx 30rpx 15rpx #ededed;
  top: 160rpx;
  display: flex;
  flex-wrap: wrap;
  position: absolute;
  background: #fff;
  left: 50%;
  margin-left: -44%;
}

.centerList {
  width: 50%;
  padding: 90rpx 0;
}

.centerList:nth-child(1) {
  border-right: 1rpx solid #F2F2F2;
  border-bottom: 1rpx solid #F2F2F2;
}

.centerList:nth-child(2) {
  border-bottom: 1rpx solid #F2F2F2;
}

.centerList:nth-child(3) {
  border-right: 1rpx solid #F2F2F2;
}

.centerListImg {
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
}

.centerList>text {
  display: block;
  color: #333;
  font-size: 28rpx;
  width: 100%;
  text-align: center;
  margin-top: 48rpx;
}
.footerImg{
  width: 100%;
  height: 164rpx;
  position: absolute;
  bottom: 0;
}